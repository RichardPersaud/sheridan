ch = 'x';
ch = 'y';
//ch = "Z";

byte smallValue;
smallValue = 78;
smallValue = 20;
//smallValue = 123455;

Circle c;
c = new Circle();
Circle c2 = new Circle();
c = new Circle();
//c = new Rectangle();

Program p; 
p = null;
p = new Program();
p = new Program();
//p = new Circle();

System.out.println(value);
