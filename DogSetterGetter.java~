public class DogSetterGetter { 
    public String name;
    public void bigBark() { 
        System.out.println(one + " says Woof! Woof!"); 
    }
    public void smallBark() { 
        System.out.println(two + " says Yip! Yip!"); 
    }
    
   // teachers instruction
   public void setName(String strName) {
        
        name = strName;
    }
    
    public String getName ()
    {
        return name;
    } 
    
    One.setName ("Thor");
    Two.setName ("Tiny");
    
    
