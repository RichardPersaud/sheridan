import java.util.*;
public SwappAlgo{
    
    
    public static void main(String[] args){
        boolean swapped; // Swap flag
        do {
            swapped = false;
            for (int i = 1; i < a.length; i++) {
// Check if we need to swap
                if (a[i-1] > a[i]) {
// Swap two array elements
                    int c = a[i];
                    a[i] = a[i-1];
                    a[i-1] = c;
                    swapped = true; // Show that swap occurred
                }
            }
        } while (swapped);
    }
}